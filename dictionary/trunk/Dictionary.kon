  <about-box>
    <image>Resources/AboutBox.png</image>
    <about-version
      font="Arial Bold"
      size="13"
      hOffset="265" vOffset="112"
      color="#FFFFFF">
      <shadow>
        <color>#000000</color>
        <hOffset>1</hOffset>
        <vOffset>1</vOffset>
      </shadow>
    </about-version>
  </about-box>
