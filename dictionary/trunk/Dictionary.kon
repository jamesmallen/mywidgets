  <preference name="revertToDefaults"
    notSaved="true"
    title="Revert to Defaults"
    type="checkbox"
    defaultValue="0"
    description="Check this box and click Save to revert to the default collection of books available."/>
    

  <preference name="selectedBook"
    hidden="true"
    defaultValue="0"
    type="text"/>
  
  <preference name="books"
    hidden="true"
    defaultValue="Dictionary%2Ecom,http%3A%2F%2Fdictionary%2Ereference%2Ecom%2Fsearch%3Fq%3D,Dictionary%2Ecom,%23483BAE|Thesaurus%2Ecom,http%3A%2F%2Fthesaurus%2Ereference%2Ecom%2Fsearch%3Fq%3D,Thesaurus%2Ecom,%23CB3434|Wikipedia%20%28English%29,http%3A%2F%2Fen%2Ewikipedia%2Eorg%2Fwiki%2F,MediaWiki,%23356E25|Wiktionary%20%28English%29,http%3A%2F%2Fen%2Ewiktionary%2Eorg%2Fwiki%2F,MediaWiki,%23896623|Merriam%2DWebster%20OnLine,http%3A%2F%2Fm%2Dw%2Ecom%2Fcgi%2Dbin%2Fdictionary%3Fbook%3DDictionary%26va%3D,m%2Dw%2Ecom,%238D306D"
